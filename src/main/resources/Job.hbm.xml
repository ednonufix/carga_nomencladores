<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cu.com.cimex.model.TCron" table="tb_cron">
        <id name="id_cron">
            <generator class="native"/>
        </id>

        <property name="nomb_cron" not-null="true" type="string"/>
        <property name="fecha_culminado" not-null="true" type="java.util.Date"/>

    </class>
</hibernate-mapping>
